<div class="container">
  <h2>Add new Restaurant</h2>

  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.submit class:"btn btn-primary"%>
  <% end %>

  <%= link_to "back", restaurants_path %>
</div>
